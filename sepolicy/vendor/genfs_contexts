# Alert Slider
genfscon sysfs /devices/platform/soc/soc:tri_state_key/hall_data_calib                                             u:object_r:vendor_sysfs_tri_state_key:s0

# Display
genfscon proc  /touchpanel                                                                                         u:object_r:proc_touchpanel:s0
genfscon sysfs /devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/dim_alpha                         u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/hbm                               u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/native_display_p3_mode            u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/native_display_srgb_color_mode    u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/notify_dim                        u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/notify_fppress                    u:object_r:vendor_sysfs_graphics:s0

# Charging
genfscon sysfs /devices/platform/soc/soc:oplus,chg_gki/power_supply/battery                                        u:object_r:vendor_sysfs_battery_supply:s0
genfscon sysfs /devices/platform/soc/soc:oplus,chg_gki/power_supply/usb                                            u:object_r:vendor_sysfs_usb_supply:s0
genfscon sysfs /devices/platform/soc/soc:oplus,chg_gki/power_supply/wireless                                       u:object_r:vendor_sysfs_usb_supply:s0

# Wakeups
genfscon sysfs /devices/platform/soc/soc:oplus,chg_gki/power_supply/battery/wakeup                                 u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/soc:oplus,chg_gki/power_supply/usb/wakeup                                     u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/soc:oplus,chg_gki/power_supply/wireless/wakeup                                u:object_r:sysfs_wakeup:s0
